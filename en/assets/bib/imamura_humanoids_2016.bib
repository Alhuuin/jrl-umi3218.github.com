@InProceedings{imamura:humanoids:2016,
  author    = {Imamura,Yumeko and Ayusawa,Ko and Endo,Yui and Yoshida,Eiichi},
  title     = {Analysis of Robotic Care Devices using Human Dynamics Simulator},
  booktitle = {IEEE-RAS International Conference on Humanoid Robots},
  year      = {2016},
  address   = {Cancun, Mexico},
  month     = {November 15-November 17},
  url       = {https://staff.aist.go.jp/e.yoshida/papers/ICORR17\_Imamura.pdf},
